                    // event handlers tracking calls
                    events: {
                        onEventLog: function (eventId, dataPoints)
                        {
                            if (eventId.indexOf('MeControl_') >= 0 && dataPoints.type)
                            {
                                if (dataPoints.type === 'qos' && dataPoints.success === '0')
                                {
                                    var eventData = {
                                        currentOperationName: eventId,
                                        isSuccess: false,
                                        latencyMs: dataPoints.duration,
                                        errorMessage: dataPoints.errorCode + (dataPoints.IDPError ? ':' + dataPoints.IDPError : ''),
                                        requestUri: dataPoints.targetUri,
                                        serviceName: controlName,
                                        operationName: eventId
                                    };
                                    vortexEvents.sendApiComplete(eventData);
                                }
                                if (dataPoints.type == 'bici' && eventId.indexOf('Ready') < 0)
                                {
                                    var content = {
                                        name: eventId,
                                        contentId: eventId,
                                        areaName: controlName,
                                        targetUri: dataPoints.targetUri
                                    };
                                    //To Webi
                                    biTracker.captureContentPageAction(content);
                                    //To WEDCS
                                    window.MscomCustomEvent(wedcsTags.ContentName, eventId, wedcsTags.TargetUri, dataPoints.destUri, wedcsTags.AreaName, containerControl, wedcsTags.ComponentName, controlName);
                                }
                            }
                        },
                        onSearch: function (searchForm)
                        {
                            var query = searchForm.q ? searchForm.q.value : "";
                            var scope = searchForm.form ? searchForm.form.value : "";
                            var destUri = searchForm.action + "?" + $(searchForm).serialize();
                            var actionName = "Search";
                            var biContent = {
                                name: actionName,
                                targetUri: destUri
                            };
                            var customData = {
